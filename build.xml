<project default="build" name='Ant test' >
		<typedef resource="net/sf/antcontrib/antlib.xml" />
	<target name="no_op">	
	</target>
	
	<target name="build">
    <echo message="the simple error message should be shown in overview page and fail the build when switch on the failure condition 'an error message is logged by build runner'" />
    <echo message="##teamcity[message text='simple error' errorDetails='simple error detail message' status='ERROR' flowId='simpleerror']" />
    
    <echo message="the error message between compilation block should be shown in overview page and fail the build even not switch on the failure condition 'an error message is logged by build runner'" />
    <echo message="##teamcity[compilationStarted compiler='msbuild' flowId='msbuild']" />
    <echo message="##teamcity[message text='error between compilation block' errorDetails='this is the error detail message between compilation block' status='ERROR' flowId='msbuild']" />
    <echo message="##teamcity[compilationFinished compiler='msbuild' flowId='msbuild']" />
	</target>

	<target name="test">
	  <echo message="this is test failure which should be shown as failed in overview page" />
    
    <echo message="##teamcity[testStarted name='MyTest.test1' flowId='test']" />
    <echo message="##teamcity[testFailed name='MyTest.test1' message='failure message' details='message and stack trace' flowId='test']" />  
    <echo message="##teamcity[testFinished name='MyTest.test1' duration='50' flowId='test']" />
    
    <echo message="Simulate parallel message" />
    <echo message="##teamcity[testSuiteStarted name='test.WebSocketPackageUnitTest.summary' flowId='WebSocketPackageUnitTest']" />
    <echo message="##teamcity[testSuiteStarted name='test.ThumbnailPreviewServiceUnitTest.summary' flowId='ThumbnailPreviewServiceUnitTest']" />
    <echo message="##teamcity[testStarted name='WebSocketPackageUnitTest.unittest.run' flowId='WebSocketPackageUnitTest']" />
    <echo message="##teamcity[testSuiteStarted name='test.AnimationPackageUnitTest.summary' flowId='AnimationPackageUnitTest']" />
    <echo message="##teamcity[testStarted name='ThumbnailPreviewServiceUnitTest.unittest.run' flowId='ThumbnailPreviewServiceUnitTest']" />
    <echo message="##teamcity[testStdErr name='WebSocketPackageUnitTest.unittest.run' flowId='WebSocketPackageUnitTest']" />
    <echo message="##teamcity[testStdOut name='ThumbnailPreviewServiceUnitTest.unittest.run' flowId='ThumbnailPreviewServiceUnitTest']" />
    <echo message="##teamcity[testStdOut name='WebSocketPackageUnitTest.unittest.run' flowId='WebSocketPackageUnitTest']" />
    <echo message="##teamcity[testStdOut name='AnimationPackageUnitTest.unittest.run' flowId='AnimationPackageUnitTest']" />
    <echo message="##teamcity[testStdErr name='AnimationPackageUnitTest.unittest.run' flowId='AnimationPackageUnitTest']" />
    <echo message="##teamcity[testFinished name='WebSocketPackageUnitTest.unittest.run' duration='50' flowId='WebSocketPackageUnitTest']" />
    <echo message="##teamcity[testStarted name='AnimationPackageUnitTest.unittest.run' flowId='AnimationPackageUnitTest']" />
    <echo message="##teamcity[testFinished name='AnimationPackageUnitTest.unittest.run' duration='50' flowId='AnimationPackageUnitTest']" />
    <echo message="##teamcity[testStdErr name='ThumbnailPreviewServiceUnitTest.unittest.run' flowId='ThumbnailPreviewServiceUnitTest']" />
    <echo message="##teamcity[testFinished name='ThumbnailPreviewServiceUnitTest.unittest.run' duration='50' flowId='ThumbnailPreviewServiceUnitTest']" />
    <echo message="##teamcity[testSuiteFinished name='test.ThumbnailPreviewServiceUnitTest.summary' flowId='ThumbnailPreviewServiceUnitTest']" />
    <echo message="##teamcity[testSuiteFinished name='test.WebSocketPackageUnitTest.summary' flowId='WebSocketPackageUnitTest']" />
    <echo message="##teamcity[testSuiteFinished name='test.AnimationPackageUnitTest.summary' flowId='AnimationPackageUnitTest']" />
	</target>
  
  <target name="java">
    <javac srcdir="src" failonerror="true" />
    <java classname="Main" classpath="src" failonerror="true">
    </java>
  </target>
</project>
